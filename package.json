{
  "name": "@slimio/markdown-dependencies",
  "version": "0.1.0",
  "description": "Create Markdown Dependencies for SlimIO projects",
  "main": "index.js",
  "scripts": {
    "prepublishOnly": "pkg-ok",
    "test": "cross-env psp && ava --verbose",
    "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=html",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SlimIO/Markdown-Dependencies.git"
  },
  "keywords": [
    "SlimIO",
    "markdown",
    "generator",
    "tool",
    "cli"
  ],
  "files": [
    "src",
    "bin"
  ],
  "author": "SlimIO",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SlimIO/Markdown-Dependencies/issues"
  },
  "homepage": "https://github.com/SlimIO/Markdown-Dependencies#readme",
  "dependencies": {
    "@slimio/npm-registry": "^0.5.0",
    "@slimio/pretty-json": "^1.3.0",
    "clipboardy": "^2.3.0",
    "dotenv": "^8.2.0",
    "kleur": "^3.0.3",
    "make-promises-safe": "^5.1.0",
    "marked": "^0.8.2",
    "sade": "^1.7.3",
    "snyk": "^1.316.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@escommunity/minami": "^1.0.0",
    "@slimio/eslint-config": "^4.0.0",
    "@slimio/psp": "^0.11.0",
    "ava": "^3.5.1",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "husky": "^4.2.3",
    "jsdoc": "^3.6.2",
    "nyc": "^15.0.0",
    "pkg-ok": "^2.3.1"
  },
  "engines": {
    "node": ">=12"
  },
  "husky": {
    "hooks": {
      "pre-push": "cross-env eslint bin/index.js && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "nyc": {
    "check-coverage": true,
    "per-file": false,
    "lines": 98,
    "statements": 98,
    "functions": 98,
    "branches": 90
  },
  "bin": {
    "mddep": "./bin/index.js"
  },
  "snyk": true
}
